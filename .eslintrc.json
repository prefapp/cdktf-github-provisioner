{
  "extends": [
    "./node_modules/gts/"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/ban-types": [
      "error",
      {
        "types": {
          "Function": false
        },
        "extendDefaults": true
      }
    ]
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "root": true
}
